configfile: "configfile.yaml"

# https://groups.google.com/forum/#!msg/snakemake/AuSI24J4sOw/CqKCa-XsDgAJ


rule create_report:
    output:
        report = os.path.join('{subworkflow_path}', 'report.html')
    shell:
        """
        cd {subworkflow_path}
        snakemake --report {output.report}
        """
